  object IBQ_Individu: TIBQuery
    Database = DMWeb.ibd_BASE
    Transaction = DMWeb.IBT_BASE
    BufferChunks = 1000
    CachedUpdates = False
    GeneratorField.ApplyOnEvent = gaeOnNewRecord
    SQL.Strings = (
      'select CLE_FICHE'
      ',NOM'
      ',PRENOM'
      ',cle_pere'
      ',cle_mere'
      ',sexe'
      ',num_sosa'
      ',ANNEE_NAISSANCE'
      ',ANNEE_DECES '
      'from INDIVIDU '
      'WHERE KLE_DOSSIER=:KLE_DOSSIER '
      'ORDER BY NOM'
      ',PRENOM'
      ',ANNEE_NAISSANCE'
      ',ANNEE_DECES '
    )
    Params = <    
